datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model HistoricalEvent {
  id          Int      @id @default(autoincrement())
  date        String
  description String
  lang        String
  category1   String
  category2   String
  granularity String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
